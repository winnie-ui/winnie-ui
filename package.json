{
  "name": "winnie-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo run build --cache-dir=.turbo",
    "biome:check": "turbo run biome:check --cache-dir=.turbo",
    "changeset": "changeset",
    "changeset:publish": "changeset publish",
    "clean": "turbo run clean && rimraf node_modules",
    "format:check": "turbo run format:check --cache-dir=.turbo",
    "format:write": "turbo run format:write --cache-dir=.turbo",
    "lint:check": "turbo run lint:check --cache-dir=.turbo",
    "lint:write": "turbo run lint:write --cache-dir=.turbo",
    "tsc": "turbo run tsc",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.9",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "rimraf": "6.0.1",
    "turbo": "2.2.3"
  },
  "packageManager": "pnpm@9.7.1"
}
