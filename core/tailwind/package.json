{
  "name": "@winnie-ui/tailwind",
  "version": "0.0.25",
  "description": "tailwind theme for winnie-ui css",
  "author": "Adam Aho",
  "type": "module",
  "main": "./src/index.css",
  "exports": {
    ".": "./src/index.css"
  },
  "files": ["src", "package.json"],
  "scripts": {
    "clean": "rimraf node_modules",
    "format:check": "biome format ./src --no-errors-on-unmatched",
    "format:write": "biome format ./src --write --no-errors-on-unmatched"
  },
  "peerDependencies": {
    "tailwindcss": "4.0.0-alpha.15"
  },
  "dependencies": {
    "@winnie-ui/css": "workspace:*"
  },
  "devDependencies": {
    "rimraf": "6.0.1"
  }
}
