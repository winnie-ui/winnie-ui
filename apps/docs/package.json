{
  "name": "docs",
  "type": "module",
  "version": "0.0.25",
  "private": true,
  "scripts": {
    "astro": "astro",
    "build": "rimraf dist .astro && astro check && astro build",
    "clean": "rimraf node_modules dist .astro",
    "dev": "astro dev",
    "dev:sst": "sst dev",
    "deploy:production": "sst deploy --stage=production",
    "deploy:staging": "sst deploy --stage=staging",
    "format:check": "biome format ./src --no-errors-on-unmatched",
    "format:write": "biome format ./src --write --no-errors-on-unmatched",
    "remove:production": "sst remove --stage=staging",
    "remove:staging": "sst remove --stage=staging",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/mdx": "4.0.2",
    "@astrojs/react": "4.1.1",
    "@fontsource/geist-mono": "5.1.0",
    "@fontsource/inter": "5.1.0",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@react-aria/utils": "3.26.0",
    "@winnie-ui/css": "workspace:*",
    "@winnie-ui/react": "workspace:*",
    "@winnie-ui/themes": "workspace:*",
    "astro": "5.0.9",
    "astro-expressive-code": "0.38.3",
    "clsx": "2.1.1",
    "cmdk": "1.0.4",
    "flexsearch": "0.7.43",
    "lucide-react": "0.468.0",
    "react": "19.0.0",
    "react-aria": "3.36.0",
    "react-aria-components": "1.5.0",
    "react-dom": "19.0.0",
    "recharts": "2.15.0"
  },
  "devDependencies": {
    "@tanstack/router-devtools": "1.91.2",
